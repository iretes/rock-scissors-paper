ctmc

const int Max;
const int N;
module  rsp

S:  [ 0..Max ]  init 30*N;
R:  [ 0..Max ]  init 50*N;
P:  [ 0..Max ]  init 20*N;


[Pp]
(P > 0) & (R > 0)
 -> (0.3) * P * R : 
(P' = P+1) & (R' = R-1);


[Pr]
(R > 0) & (S > 0)
 -> (0.2) * R * S : 
(R' = R+1) & (S' = S-1);


[Ps]
(P > 0) & (S > 0)
 -> (0.5) * S * P : 
(P' = P-1) & (S' = S+1);





endmodule
